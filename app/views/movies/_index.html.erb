<div class="container">
  <div class="w-100 list-movies">
    <% if @list_movies_of_users.present? &&  @list_movies_of_users.size > 0 %>
      <% @list_movies_of_users.each do |movie| %>
        <% cache movie do %>
          <%= render partial: 'movies/show', locals: { movie: movie } %>
        <% end %>
      <% end %>
      <div class="mt-5 mb-5 w-100 d-flex-center">
        <%= paginate @list_movies_of_users %>
      </div>
    <% else %>
      Not Video Available
    <% end %>
  </div>
</div>